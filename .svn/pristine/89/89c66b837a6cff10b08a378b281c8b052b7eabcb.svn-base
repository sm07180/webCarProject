package tip;

import java.sql.Date;

public class TipVO {
	private String user_id,user_name;
	private int tt_id,no,readcnt;
	private String tt_title,tt_content,title_name;
	private Date tt_date;
	
	
	
	public String getTitle_name() {
		return title_name;
	}

	public void setTitle_name(String title_name) {
		this.title_name = title_name;
	}

	public String getUser_name() {
		return user_name;
	}

	public void setUser_name(String user_name) {
		this.user_name = user_name;
	}
	
	public int getReadcnt() {
		return readcnt;
	}


	public void setReadcnt(int readcnt) {
		this.readcnt = readcnt;
	}


	public int getTt_id() {
		return tt_id;
	}
	
	
	public String getUser_id() {
		return user_id;
	}
	public void setUser_id(String user_id) {
		this.user_id = user_id;
	}
	public void setTt_id(int tt_id) {
		this.tt_id = tt_id;
	}
	public int getNo() {
		return no;
	}
	public void setNo(int no) {
		this.no = no;
	}
	public String getTt_title() {
		return tt_title;
	}
	public void setTt_title(String tt_title) {
		this.tt_title = tt_title;
	}
	public String getTt_content() {
		return tt_content;
	}
	public void setTt_content(String tt_content) {
		this.tt_content = tt_content;
	}
	public Date getTt_date() {
		return tt_date;
	}
	public void setTt_date(Date tt_date) {
		this.tt_date = tt_date;
	}
	
	
	
	
	
}
